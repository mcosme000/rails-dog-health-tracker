<main>

  <div class="container">

    <section>
      <div class="dogs-container flex-basic">
        <% @dogs.each do |dog| %>
          <%=  link_to dog_path(dog) do %>
            <div class="avatar"></div>
          <% end %>
        <% end %>
      </div>
    </section>

    <%= render "reminders" %>

    <%= render "actions" %>

  </div>

</main>
