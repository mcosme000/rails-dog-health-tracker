<main>
  <div class="container">
    <p><%= link_to "Back", establishments_path %></p>

    <p>Register a new establishment</p>

    <%= simple_form_for @establishment do |f| %>
      <%= f.input :name %>
      <%= f.input :category %>
      <%= f.input :address %>
      <%= f.input :phone_number %>
      <%= f.button :submit, class: "btn" %>
    <% end %>

  </div>
</main>
